{
  "name": "yunque",
  "version": "0.1.1",
  "description": "Simple scaffolding tools.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yunquejs/yunque.git"
  },
  "keywords": [
    "yunque",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/yunquejs/yunque/issues"
  },
  "homepage": "https://github.com/yunquejs/yunque#readme",
  "author": "君惜",
  "license": "MIT",
  "scripts": {
    "copyfiles": "node ./scripts/copyfiles.js",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@yunquejs/release": "workspace:*",
    "@yunquejs/fabric": "workspace:*",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.14.39",
    "conventional-changelog-cli": "^2.1.1",
    "jest": "^26.1.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.1.1",
    "tsup": "^5.11.6",
    "typescript": "^4.2.3",
    "copyfiles": "^2.4.1",
    "yorkie-pnpm": "^2.0.1"
